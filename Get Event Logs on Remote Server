#Get error/warning/critical entries from a remote computer from the past 24 hours and save to a .txt
Get-EventLog -ComputerName "ServerName" -LogName System -After (get-date).AddDays(-1) | Where-Object {$_.EntryType -like 'Error' -or $_.EntryType -like 'Warning' -or $_.EntryType -like ‘Critical’} > c:\path\File.txt
